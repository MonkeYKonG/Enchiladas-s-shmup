<panel x="%($(window.width)$ / 2)%" y="%($(window.height)$ / 2)%">
  <title>
    <text>Jouer</text>
    <font>Default</font>
    <color red="12" green="178" blue="16"></color>
  </title>
  <background>
    <texture>panel_background.png</texture>
  </background>
  <border width="544" height="704" tileX="32" tileY="32">
    <texture>panel_border.png</texture>
    <corner x="0" y="0"></corner>
    <outline x="64" y="0"></outline>
  </border>
  <buttons>
    <button class="sprite" x="0" y="-150">
      <title>
        <text>Solo</text>
        <font>Default</font>
        <color red="0" green="0" blue="0"></color>
      </title>
      <texture>button.png</texture>
      <animations>
        <animation key="default">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="0" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_mouse_over">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="200" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_click">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="400" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
      </animations>
    </button>
    <button class="sprite" x="0" y="0">
      <title>
        <text>Multi</text>
        <font>Default</font>
        <color red="0" green="0" blue="0"></color>
      </title>
      <texture>button.png</texture>
      <animations>
        <animation key="default">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="0" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_mouse_over">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="200" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_click">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="400" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
      </animations>
    </button>
    <button class="sprite" x="0" y="150">
      <title>
        <text>Retour</text>
        <font>Default</font>
        <color red="0" green="0" blue="0"></color>
      </title>
      <texture>button.png</texture>
      <animations>
        <animation key="default">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="0" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_mouse_over">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="200" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
        <animation key="on_click">
          <framerateMax>0</framerateMax>
          <loop>false</loop>
          <rects>
            <rect x="400" y="0" tileX="200" tileY="100"></rect>
          </rects>
        </animation>
      </animations>
    </button>
  </buttons>
</panel>